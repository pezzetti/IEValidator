{
  "name": "pezzetti/IEValidator",
  "homepage": "https://github.com/pezzetti/IEValidator",
  "license": "MIT",
  "type": "library",
  "description": "Biblioteca PHP para validar e imprimir  inscrições estaduais, seguindo as regras da SEFAZ. http://www.sintegra.gov.br/insc_est.html",
  "require": {
    "php": ">=5.4.16"
  },
  "authors": [
    {
      "name": "Rafael Pezzetti",
      "email": "rafael.pezzetti@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "codacy/coverage": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Pezzetti\\InscricaoEstadual\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pezzetti\\InscricaoEstadual\\": "tests/"
    }
  }
}
